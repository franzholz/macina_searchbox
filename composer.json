{
  "name": "jambagecom/macina-searchbox",
  "type": "typo3-cms-extension",
  "description": "This extension offers the possibility to add a configuration for the searchbox as a TypoScript plugin on all pages which submits the searchvalue to the Indexed Search Engine plugin.",
  "keywords": [
    "TYPO3",
    "extension",
    "indexed",
    "search"
  ],
  "support": {
    "issues": "https://github.com/franzholz/macina_searchbox/issues"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    },
    {
      "name": "Wolfgang Becker",
      "role": "Developer"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^12.4 || ^13.4"
  },
  "suggest": {
    "typo3/cms-indexed-search": "^12.4 || ^13.4"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\MacinaSearchbox\\": "Classes"
    }
  },
  "replace": {
    "typo3-ter/macina-searchbox": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "macina_searchbox"
    }
  }
}
